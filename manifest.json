{
  "manifest_version": 3,
  "name": "GRE words",
  "version": "0.0.0.1",
  "description": "Learn GRE words one by one",
  "chrome_url_overrides":{
    "newtab": "newTab.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_title": "GRE Words",
    "default_popup": "popup.html"
  },
  "icons": { "16": "/Icons/icon16.png",
           "48": "/Icons/icon48.png",
          "128": "/Icons/icon128.png" }
}
